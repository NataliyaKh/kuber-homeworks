[req]
default_bits       = 2048
prompt             = no
default_md         = sha256
req_extensions     = req_ext
distinguished_name = dn

[dn]
C  = RU
ST = Some-State
L  = Some-City
O  = MyOrganization
OU = MyUnit
CN = myapp.example.com

[req_ext]
subjectAltName = @alt_names

[alt_names]
DNS.1 = myapp.example.com
